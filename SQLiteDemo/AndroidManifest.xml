    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="13"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.sqlitedemo.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.sqlitedemo.MyTasks"></activity>
        <activity android:name="com.example.sqlitedemo.activity_addtask"></activity>
        <activity android:name="com.example.sqlitedemo.Completed"></activity>
        <activity android:name="com.example.sqlitedemo.DBHepler"></activity>
        <activity android:name="com.example.sqlitedemo.DBAdapter"></activity>
        <activity android:name="com.example.sqlitedemo.Adapter"></activity>
    </application>

</manifest>